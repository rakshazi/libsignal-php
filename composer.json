{
    "name": "rakshazi/libsignal-php",
    "type": "library",
    "license": "GPL-3.0-only",
    "description": "Fork of mgp25/libsignal-php. This is a php port of libsignal-java originally written by Moxie Marlinspike",
    "replace": {
        "mgp25/libsignal-php": "dev-master"
    },
    "require": {
        "php": ">= 5.6",
        "ext-curve25519": "*",
        "ext-protobuf": "*",
        "ext-crypto": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.1"
    },
    "autoload": {
        "psr-4": {
            "Libsignal\\": "src/",
            "Whispertext\\": "src/Whispertext/",
            "Localstorage\\": "src/Localstorage/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Libsignal\\Tests\\": "tests/"
        }
    }
}
